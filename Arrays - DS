#include <bits/stdc++.h>

using namespace std;



// Complete the reverseArray function below.


int main()
{
    
    int arr_count;
    cin >> arr_count;
  vector <int> arr(arr_count);

    for (int i = 0; i < arr_count; i++) 
    {
        cin>>arr[i] ;
    }


    for (int i = arr.size()-1; i >= 0; i--) {
        cout << arr[i]<<" ";

    }

    return 0;
}
