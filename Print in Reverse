

// Complete the reversePrint function below.

/*
 * For your reference:
 *
 * SinglyLinkedListNode {
 *     int data;
 *     SinglyLinkedListNode* next;
 * };
 *
 */
void reversePrint(SinglyLinkedListNode* head) 
{
vector <int> arr;
if(head==NULL)
return;
while(head!=NULL)
{
    arr.push_back(head->data);
    head=head->next;
}
for(int i=arr.size()-1;i>=0;i--)
{
    cout<<arr[i]<<"\n";

}
}

