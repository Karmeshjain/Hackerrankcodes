#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<ctype.h>
int main()
{
    int i;
    char str[1000];
    scanf("%[^\n]",str);
    for(i=0;str[i]!=NULL;i++)
    {
        if(isspace(str[i]))
        printf("\n");
        else
        printf("%c",str[i]);
    }
    return 0;
}
