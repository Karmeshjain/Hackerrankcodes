#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<ctype.h>
int main() {

    char str[1000];
    int l,i,j;
    static l1[10];
    scanf("%s",str);
    l=strlen(str);
    for(i=0;i<l;i++)
    {
        if(isdigit(str[i]))
       {if(str[i]=='0')
        l1[0]++; 
         else if(str[i]=='1')
        l1[1]++; 
          else if(str[i]=='2')
        l1[2]++; 
          else if(str[i]=='3')
        l1[3]++; 
          else if(str[i]=='4')
        l1[4]++; 
          else if(str[i]=='5')
        l1[5]++; 
          else if(str[i]=='6')
        l1[6]++; 
          else if(str[i]=='7')
        l1[7]++; 
          else if(str[i]=='8')
        l1[8]++; 
           else if(str[i]=='9')
        l1[9]++; 
       }
         }
         for(j=0;j<10;j++)
         {printf("%d ",l1[j]);}


    return 0;
}
