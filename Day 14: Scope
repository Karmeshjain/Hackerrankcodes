
    Difference(vector <int> a)
    {
        elements=a;
    }  
    void computeDifference()
    {
        maximumDifference=-99999;
        int k;
        for(int i=0;i<elements.size();i++)
        {
            for(int j=0;j<elements.size();j++)
            {   
                
                k=elements[i]-elements[j];
                k=abs(k);
                if(maximumDifference<k)
                  maximumDifference=k;
            }
        }
    }   
