import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    
     Scanner line = new Scanner(System.in);
     int counter=1;
    while (line.hasNextLine()) 
    {
String line1 = line.nextLine();
System.out.println(counter  +" "+line1);
counter++;
    }
}}
