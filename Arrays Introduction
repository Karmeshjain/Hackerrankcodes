#include<iostream>
using namespace std;
int main() 
{
int n,j,i,temp;
cin>>n;
int arr[n];
for(i=0;i<n;i++)
{cin>>arr[i];}
j=i-1;
i=0;
while(i<j)
    {
        temp=arr[i];
        arr[i]=arr[j];
        arr[j]=temp;
        i++;
        j--;
    }
    for(i=0;i<n;i++)
{cout<<arr[i]<<" ";}
return 0;
}
