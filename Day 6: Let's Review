#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n;
    cin>>n;
    string s[n];
    for(int j=0;j<n;j++)
    cin>>s[j];
    for(int k=0;k<n;k++)
    {
        string a,b;
    for(int i=0;i<s[k].length();i++)
    {
        if(i%2==0)
         a=a+s[k][i];
        else
        b=b+s[k][i];
    }
    cout<<a<<" "<<b<<"\n";
    }    return 0;
}
