#include<iostream>
using namespace std;
int main()
{
    int n,i;
    float cps=0,cze=0,cne=0;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    cin>>a[i];
    for(i=0;i<n;i++)
    {
        if(a[i]>0)
        cps++;
        else if(a[i]<0)
        cne++;
        else 
        cze++;
    } 
cout<<cps/n<<"\n"<<cne/n<<"\n"<<cze/n<<"\n";
}
