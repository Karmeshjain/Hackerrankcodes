#include <iostream>
#include <bits/stdc++.h>
using namespace std;

class Solution 
{
    public:
    stack<char> s;
    queue<char> q;
    void pushCharacter(char ch)
    {
        s.push(ch);
    }
    void enqueueCharacter(char ch)
    {
        q.push(ch);
    }
    char popCharacter()
    {
        char s2=s.top();

        s.pop();
        return s2;
    }
    char dequeueCharacter()
    {
        char s3=q.front();
      q.pop();
      return s3;
    }
    //Write your code here

};

